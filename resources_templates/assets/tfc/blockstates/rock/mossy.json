{
  "forge_marker": 1,
  "defaults": {
    "model": "tfc:mossy",
    "uvlock": true
  },
  "variants": {
    "stonetype": {<%for (i=0; i < types.size; i++) { %>
      "${types[i]}": {
          "textures": {
            "all": "tfc:blocks/rock/$texture_variant/${types[i]}",
            "overlay": "tfc:blocks/rock/overlay/mossy/$texture_variant"
      }}<% if(i < types.size - 1) { %>, <%}} %>
    }
  }
}
