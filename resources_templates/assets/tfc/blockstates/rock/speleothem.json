{
  "forge_marker": 1,
  "defaults": {
    "uvlock": true,
    "transform": "forge:default-block",
    "model": "tfc:speleothem_medium"
  },
  "variants": {
    "size": {
      "small": {
        "model": "tfc:speleothem_small"
      },
      "medium": {
        "model": "tfc:speleothem_medium"
      },
      "big": {
        "model": "tfc:speleothem_big"
      }
    },
    "stonetype": {<%for (i=0; i < types.size; i++) { %>
      "${types[i]}": {
        "textures": {
          "all": "tfc:blocks/rock/$texture_variant/${types[i]}"
        }}<% if(i < types.size - 1) { %>, <%}
      } %>
    }
  }
}
