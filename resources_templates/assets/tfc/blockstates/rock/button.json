{
	"forge_marker": 1,
	"defaults": {
		"model": "button_inventory",
		"uvlock": true
	},
	"variants": {
		"facing": {
			"up": {},
			"down": {
				"x": 180
			},
			"east": {
				"x": 90,
				"y": 90
			},
			"west": {
				"x": 90,
				"y": 270
			},
			"south": {
				"x": 90,
				"y": 180
			},
			"north": {
				"x": 90
			}
		},
		"powered": {
			"false": {
				"model": "button"
			},
			"true": {
				"model": "button_pressed"
			}
		},
		"stonetype": {<%for (i=0; i < types.size; i++) { %>
      "${types[i]}": {
        "textures": {
          "texture": "tfc:blocks/rock/$texture_variant/${types[i]}"
      }}<% if(i < types.size - 1) { %>, <%}} %>
    }<%for (i=0; i < types.size; i++) { %>,
	  "inventory=${types[i]}": [{
      "textures": {
        "texture": "tfc:blocks/rock/$texture_variant/${types[i]}"
      }}
    ]<%} %>
  }
}
