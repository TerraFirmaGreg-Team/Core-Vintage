{
	"forge_marker": 1,
	"defaults": {
		"model": "button_inventory",
		"uvlock": true
	},
	"variants": {
		"facing": {
			"up": {},
			"down": {
				"x": 180
			},
			"east": {
				"x": 90,
				"y": 90
			},
			"west": {
				"x": 90,
				"y": 270
			},
			"south": {
				"x": 90,
				"y": 180
			},
			"north": {
				"x": 90
			}
		},
		"powered": {
			"false": {
				"model": "button"
			},
			"true": {
				"model": "button_pressed"
			}
		},
		"rocktype": {<%for (i=0; i < rock_type.size; i++) { %>
      "${rock_type[i]}": {
        "textures": {
          "texture": "tfc:blocks/rock/raw/${rock_type[i]}"
      }}<% if(i < rock_type.size - 1) { %>, <%}} %>
    }<%for (i=0; i < rock_type.size; i++) { %>,
	  "inventory=${rock_type[i]}": [{
      "textures": {
        "texture": "tfc:blocks/rock/raw/${rock_type[i]}"
      }}
    ]<%} %>
  }
}
