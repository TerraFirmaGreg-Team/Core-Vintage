{
  "forge_marker": 1,
  "defaults": {
    "model": "tfc:loose_rock",
    "uvlock": false
  },
  "variants": {
    "axis": {
      "north": {
        "y": 0
      },
      "east": {
        "y": 90
      },
      "south": {
        "y": 180
      },
      "west": {
        "y": 270
      }
    },
    "rocktype": {<%for (i=0; i < rock_type.size; i++) { %>
      "${rock_type[i]}": {
          "textures": {
            "stone": "tfc:blocks/rock/raw/${rock_type[i]}"
      }}<% if(i < rock_type.size - 1) { %>, <%}} %>
    }
  }
}
