{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": {
        "model": "tfg:flora/plant/fountain_grass_shortest"
      }
    }, {
      "when": {
        "age": "1"
      },
      "apply": {
        "model": "tfg:flora/plant/fountain_grass_shorter"
      }
    }, {
      "when": {
        "age": "2"
      },
      "apply": {
        "model": "tfg:flora/plant/fountain_grass_short"
      }
    }, {
      "when": {
        "age": "3"
      },
      "apply": {
        "model": "tfg:flora/plant/fountain_grass"
      }
    }
  ]
}
